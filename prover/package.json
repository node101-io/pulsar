{
    "name": "pulsar-prover",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "test": "npm run test",
        "build": "tsc",
        "proto:gen": "buf generate ../chain/proto",
        "start-mock": "npm run build && node dist/src/mock/pulsarNode.js",
        "worker:settlement": "npm run build && node --experimental-vm-modules dist/src/worker/settlementWorker.js",
        "worker:merge": "npm run build && node --experimental-vm-modules dist/src/worker/mergeWorker.js",
        "worker:reduce": "npm run build && node --experimental-vm-modules dist/src/worker/reducerWorker.js",
        "manager": "npm run build && node dist/src/nodeManager.js",
        "cache": "node --experimental-vm-modules dist/src/runCache.js",
        "run-cache": "npm run build && i=1; while [ $i -le 10 ]; do echo \"Run #$i\"; npm run cache || exit 1; i=$((i+1)); done",
        "cli": "npm run build && node dist/src/cli.js",
        "clean": "rimraf build coverage node_modules package-lock.json"
    },
    "keywords": [
        "pulsar"
    ],
    "author": "",
    "license": "MIT",
    "description": "",
    "dependencies": {
        "@bufbuild/protobuf": "^2.7.0",
        "@cosmjs/crypto": "^0.36.0",
        "@cosmjs/encoding": "^0.36.0",
        "@cosmjs/proto-signing": "^0.36.0",
        "@cosmjs/stargate": "^0.36.0",
        "@grpc/grpc-js": "^1.13.4",
        "@grpc/proto-loader": "^0.7.15",
        "@types/google-protobuf": "^3.15.12",
        "@types/inquirer": "^9.0.9",
        "bullmq": "^5.56.0",
        "cosmjs-types": "^0.10.1",
        "cosmwasm": "^1.1.1",
        "dotenv": "^16.5.0",
        "google-protobuf": "^3.21.4",
        "grpc-js-reflection-client": "^1.3.0",
        "inquirer": "^12.9.4",
        "ioredis": "^5.6.1",
        "mongodb": "^6.17.0",
        "node-fetch": "^3.3.2",
        "pulsar-contracts": "^0.3.9",
        "rimraf": "^6.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "@bufbuild/buf": "^1.47.2",
        "@types/express": "^5.0.2",
        "@types/node": "^24.0.10",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "o1js": "^2.7.0"
    }
}
